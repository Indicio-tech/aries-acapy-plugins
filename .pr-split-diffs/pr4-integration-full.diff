+â”‚   â””â”€â”€ x509.conf             # walt.id trust anchor config
+â”œâ”€â”€ helpers/                   # Test utility modules
+â”‚   â”œâ”€â”€ acapy-client.ts       # ACA-Py admin API wrapper
+â”‚   â”œâ”€â”€ url-encoding.ts       # Base64url encoding for URLs
+â”‚   â””â”€â”€ wallet-factory.ts     # User/wallet creation helpers
+â”œâ”€â”€ tests/                     # Test specifications
+â”‚   â”œâ”€â”€ mdoc-issuance.spec.ts
