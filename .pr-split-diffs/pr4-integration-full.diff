+            ],
+            "type": ["VerifiableCredential", "UniversityDegreeCredential"],
+        },
+    )
+    supported_cred_id = supported["supported_cred_id"]
+
+    # Create issuer DID
+    did_result = await acapy_issuer_admin.post(
+        "/did/jwk/create",
+        json={"key_type": "p256"},
+    )
